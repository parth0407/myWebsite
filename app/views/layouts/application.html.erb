<!DOCTYPE html>
<html>
<head>
  <title>BazaarFeeds</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="navbar navbar-fixed-top navbar-default">
    <div class="container">
      <%= link_to "BazaarFeeds", "", class: "btn btn-lg btn-success", id: "logo" %>
      <div class="nav_item">
        <%= link_to "CLOTHING", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "SHOES", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "ACCESSORIES", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "SALE", "", class: "btn btn-default btn-warning" %>
      </div>
      
    </div>
  </header>
  <div class ="container">
      <%= yield %>
      <div class="footer">
        <p>&copy; BazaarFeeds 2015</p>
      </div>
      
  </div>
</body>
</html>
